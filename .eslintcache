ementById('article-list').scrollIntoView(); \n        } else {\n            return () => { };\n        }\n    }\n\n    useEffect(() => {\n        let isSubscribed = true;\n        FetchArticleCommonList(url).then(resp => {\n            if (isSubscribed) {\n                setArticles(resp.results);\n                setNextUrl(resp.next);\n                setPrevUrl(resp.previous);\n                setArticlesCount(resp.count);\n            }\n        });\n        return () => isSubscribed = false;\n    }, [url]);\n\n    return (\n        <div id=\"article-list\">\n            {articles.map((item, i) => (\n                <ArticleListItem key={i} item={item} />\n            ))}\n            {nextUrl && <Pagination\n                handleNext={handleNext}\n                handlePrev={handlePrev}\n            />}\n        </div>\n    );\n}\n\nexport default ArticleList;","/home/roberto/devel/javascript/comunidad-lsa/src/Containers/Common/Avatar.jsx",[],"/home/roberto/devel/javascript/comunidad-lsa/src/Containers/Foro/TopicItem/TopicItem.jsx",[],"/home/roberto/devel/javascript/comunidad-lsa/src/Services/Blog/PostCommentArticle.js",[],"/home/roberto/devel/javascript/comunidad-lsa/src/Services/Forum/AddCommentToPost.js",[],"/home/roberto/devel/javascript/comunidad-lsa/src/Services/User/FetchUserDetail.js",[],"/home/roberto/devel/javascript/comunidad-lsa/src/Services/User/FetchAvatar.js",[],"/home/roberto/devel/javascript/comunidad-lsa/src/Services/Blog/FetchArticleCommonList.js",[],"/home/roberto/devel/javascript/comunidad-lsa/src/Containers/Common/Pagination.jsx",[],"/home/roberto/devel/javascript/comunidad-lsa/src/Containers/Blog/ArticleListItem/ArticleListItem.jsx",["325"],"import { Link } from 'react-router-dom';\nimport { useEffect, useState } from \"react\";\nimport Avatar from '../../Common/Avatar';\nimport FetchUserDetail from '../../../Services/User/FetchUserDetail';\nimport FetchCommentsOfArticle from '../../../Services/Blog/FetchCommentsOfArticle';\nimport { Fa